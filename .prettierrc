{
  "$schema": "https://json.schemastore.org/prettierrc.json",
  "semi": true,
  "tabWidth": 2,
  "printWidth": 100,
  "useTabs": false,
  "singleQuote": false,
  "bracketSpacing": false,
  "endOfLine": "auto",
  "arrowParens": "always",
  "trailingComma": "all",
  "plugins": ["prettier-plugin-organize-imports", "prettier-plugin-tailwindcss"]
}
